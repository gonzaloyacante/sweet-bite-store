# Server
PORT=3000
NODE_ENV=development

# Database (Railway)
DATABASE_URL=mysql://root:password@localhost:3306/sweet_bite

# CORS
CORS_ORIGINS=http://localhost:5173,https://sweet-bite-store.vercel.app

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_EMAIL=your_email@gmail.com
SMTP_PASSWORD=your_app_password

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
